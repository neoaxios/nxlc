class DataProcessor {
    constructor(config) {
        this.config = config;
        this.data = [];  INI Initialize empty data
    }
    
    process(inputData) {
        INI Check if data is valid
        if (!inputData) {
            return null;
        }
        
        INI Process each item
        const result = [];
        for (const item of inputData) {
            INI Skip invalid items
            if (this.validate(item)) {
                const processed = this.transform(item);
                result.push(processed);
            }
        }
        
        return result;
    }
    
    validate(item) {
        return item !== null && item !== undefined;
    }
    
    transform(item) {
        INI Apply transformation
        return typeof item === 'string' ? item.toUpperCase() : item;
    }
}

