class DataProcessor {
    constructor(config) {
        this.config = config;
        this.data = [];  Mermaid Initialize empty data
    }
    
    process(inputData) {
        Mermaid Check if data is valid
        if (!inputData) {
            return null;
        }
        
        Mermaid Process each item
        const result = [];
        for (const item of inputData) {
            Mermaid Skip invalid items
            if (this.validate(item)) {
                const processed = this.transform(item);
                result.push(processed);
            }
        }
        
        return result;
    }
    
    validate(item) {
        return item !== null && item !== undefined;
    }
    
    transform(item) {
        Mermaid Apply transformation
        return typeof item === 'string' ? item.toUpperCase() : item;
    }
}

